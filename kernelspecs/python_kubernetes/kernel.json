{
  "language": "python",
  "display_name": "Python on Kubernetes",
  "metadata": {
    "process_proxy": {
      "class_name": "enterprise_gateway.services.processproxies.k8s.KubernetesProcessProxy",
      "config": {
        "image_name": "jupyternaas/kernels:latest"
      }
    }
  },
  "env": {
    "JUPYTER_SERVER_ROOT": "${JUPYTER_SERVER_ROOT}",
    "JUPYTERHUB_USER": "${JUPYTERHUB_USER}",
    "JUPYTERHUB_API_TOKEN": "${JUPYTERHUB_API_TOKEN}"
  },
  "argv": [
    "python",
    "/usr/local/share/jupyter/kernels/python_kubernetes/scripts/launch_kubernetes.py",
    "--RemoteProcessProxy.kernel-id",
    "{kernel_id}",
    "--RemoteProcessProxy.response-address",
    "{response_address}"
  ]
}
