{
  "language": "python",
  "display_name": "naas - small",
  "metadata": {
    "process_proxy": {
      "class_name": "enterprise_gateway.services.processproxies.k8s.KubernetesProcessProxy",
      "config": {
        "image_name": "jupyternaas/kernels:latest"
      }
    }
  },
  "env": {
    "KERNEL_JUPYTER_SERVER_ROOT": "${KERNEL_JUPYTER_SERVER_ROOT}",
    "KERNEL_JUPYTERHUB_USER": "${KERNEL_JUPYTERHUB_USER}",
    "KERNEL_JUPYTERHUB_API_TOKEN": "${KERNEL_JUPYTERHUB_API_TOKEN}"
  },
  "argv": [
    "python",
    "/usr/local/share/jupyter/kernels/naas_small/scripts/launch_kubernetes.py",
    "--RemoteProcessProxy.kernel-id",
    "{kernel_id}",
    "--RemoteProcessProxy.response-address",
    "{response_address}"
  ]
}